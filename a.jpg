<script>
	$( window ).load(function() {
		//플래시가 불가한 경우
		$("#sn_banner_side_top").html("<iframe class='adframe lazy' frameborder='0'  scrolling='no' width='250' height='250' data-src='https://img.sbs.co.kr/news/ad/delay_redirect.html?time=1&URL=show_ad.html?ADURL=//adservice.sbs.co.kr/NetInsight/js/sbsbanner/sbshomepage/newsend_web@newsend_web'  style='border:none; overflow:hidden;' title='오른쪽 상단 광고'>오른쪽 상단 광고</iframe>");
		$("#sn_banner_side_bottom").html("<iframe class='adframe lazy' frameborder='0'  scrolling='no' width='250' height='300' data-src='https://img.sbs.co.kr/news/ad/delay_redirect.html?time=1&URL=show_ad.html?ADURL=//adservice.sbs.co.kr/NetInsight/js/sbsbanner/sbshomepage/newsend_web@newsend_shoppingbox_web'   style='border:none; overflow:hidden;' title='오른쪽 하단 광고'>오른쪽 하단 광고</iframe>");

		var url = "https://api-gw.sbsdlab.co.kr/v1/news_front_api/keynews/";
		if(isLowMSIE()){ 	url = '/news/ajax/keyNewsList.do';}
		
		function getKeyNewsList(){
			$.ajax({
				data : {},
				async : true,
				dataType : "json",
				type : "GET",
				url : url,
				success : function(result){
					
					var html = '',
					cnt = 8;
					
					if(result.length === 0) return;
					cnt = (result.length > cnt) ? cnt : result.length;
					var oaid = (typeof(newsId) == "undefined" || newsId == null || newsId == '' ) ? '' : newsId;

					for(var i = 0; i < cnt; i++) {
						html += '<li>';
						html += '<a href="/news/endPage.do?news_id=' + result[i].newsId + '&oaid=' + oaid + '&amp;plink=POP&amp;cooper=SBSNEWSEND" class="mnl_nlist_tit newslink" title="'+result[i].itmNm.replace(/\"/gi, "&#34;")+'">';
						html += '<p class="desc">';
						html += '	<span class="num">' + (i+1) + '.</span>';
						html += '	<strong>' + result[i].itmNm + '</strong>';
						html += '</p></a></li>';
					}

					$('ul.type_ranking').append(html);
				},
				error : function(err){
					var html = '<li><a href="#"><strong class="wtl_tit">관련 내용이 없습니다.</strong></a></li>';
					$('ul.type_ranking').append(html);

				}
			});
		}
		getKeyNewsList();

		setTimeout(function(){
			try { 		$("#sn_banner_side_top").find("iframe.lazy").Lazy({bind: "event",delay: 500});}catch(err){  }
		}, 500);
		setTimeout(function(){
			try { 		$("#sn_banner_side_bottom").find("iframe.lazy").Lazy({bind: "event",delay: 500});}catch(err){  }
		}, 500);

		
		setTimeout(function(){$(".ad_area").css("background","none")},1000);
	});	
</script>
